<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="address" class="com.mayer.spring.beans.autowire.Address" p:city="Zhoushan" p:street="Tongjilu">
	</bean>
	
	<bean id="car" class="com.mayer.spring.beans.autowire.Car" p:brand="Nissan" p:price="233333"></bean>
	
	
	
	<bean id="person" class="com.mayer.spring.beans.autowire.Person" p:name="Tokiwa" autowire="byName">
		
	</bean>

</beans>
